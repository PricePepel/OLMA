    {
      "name": "Authentication",
      "item": [
        {
          "name": "Sign Up",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "apikey",
                "value": "{{supabase_anon_key}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@example.com\",\n  \"password\": \"password123\",\n  \"options\": {\n    \"data\": {\n      \"full_name\": \"Test User\",\n      \"username\": \"testuser\"\n    }\n  }\n}"
            },
            "url": {
              "raw": "{{supabase_url}}/auth/v1/signup",
              "host": ["{{supabase_url}}"],
              "path": ["auth", "v1", "signup"]
            }
          }
        },
        {
          "name": "Sign In",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
